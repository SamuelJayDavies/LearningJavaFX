<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="350.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Pane layoutX="14.0" layoutY="14.0" prefHeight="148.0" prefWidth="572.0" style="-fx-border-color: black;">
         <children>
            <Label alignment="CENTER" prefHeight="148.0" prefWidth="572.0" text="Congrats on the win!">
               <font>
                  <Font name="Comic Sans MS" size="40.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Pane layoutX="14.0" layoutY="170.0" prefHeight="79.0" prefWidth="572.0" style="-fx-border-color: black;">
         <children>
            <Label alignment="CENTER" prefHeight="79.0" prefWidth="572.0" text="Would you like to play again!">
               <font>
                  <Font name="Comic Sans MS" size="25.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Pane layoutX="14.0" layoutY="258.0" prefHeight="79.0" prefWidth="572.0" style="-fx-border-color: black;">
         <children>
            <Button fx:id="noBtn" layoutX="451.0" layoutY="11.0" mnemonicParsing="false" onAction="#switchToModeSelect" prefHeight="54.0" prefWidth="107.0" text="No" />
            <Button fx:id="yesBtn" layoutX="14.0" layoutY="13.0" mnemonicParsing="false" onAction="#switchToShed" prefHeight="54.0" prefWidth="107.0" text="Yes" />
            <Button fx:id="moreBtn" layoutX="233.0" layoutY="11.0" mnemonicParsing="false" prefHeight="54.0" prefWidth="107.0" text="More Difficult" />
         </children>
      </Pane>
   </children>
</AnchorPane>
